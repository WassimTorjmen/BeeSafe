<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".home">

    <androidx.core.widget.NestedScrollView
        android:layout_width="413dp"
        android:layout_height="481dp"
        android:fillViewport="true"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.624">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <androidx.cardview.widget.CardView
                android:layout_width="409dp"
                android:layout_height="wrap_content">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="horizontal">

                        <LinearLayout
                            android:layout_width="159dp"
                            android:layout_height="123dp"
                            android:layout_marginLeft="20dp"
                            android:layout_marginBottom="15dp"
                            android:orientation="vertical">

                            <ImageView
                                android:id="@+id/c1"
                                android:layout_width="match_parent"
                                android:layout_height="90dp"
                                app:srcCompat="@drawable/motion_sensor" />

                            <TextView
                                android:id="@+id/text22View2"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:text="Sensor C1"
                                android:textAlignment="center"
                                android:textColor="#131313"
                                android:textSize="16sp"
                                android:textStyle="bold" />
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="159dp"
                            android:layout_height="123dp"
                            android:layout_marginLeft="60dp"
                            android:layout_marginBottom="15dp"
                            android:orientation="vertical">

                            <ImageView
                                android:id="@+id/c2"
                                android:layout_width="120dp"
                                android:layout_height="92dp"
                                android:layout_gravity="center"
                                app:srcCompat="@drawable/motion_sensor" />

                            <TextView
                                android:id="@+id/te2xtView3"

                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:text="Sensor C2"
                                android:textAlignment="center"
                                android:textColor="#131313"
                                android:textSize="16sp"
                                android:textStyle="bold" />
                        </LinearLayout>
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="horizontal">

                        <LinearLayout
                            android:layout_width="159dp"
                            android:layout_height="123dp"
                            android:layout_marginLeft="20dp"
                            android:layout_marginBottom="15dp"
                            android:orientation="vertical">

                            <ImageView
                                android:id="@+id/c3"
                                android:layout_width="match_parent"
                                android:layout_height="90dp"
                                app:srcCompat="@drawable/motion_sensor" />

                            <TextView
                                android:id="@+id/textVi2ew2"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:text="Sensor C3"
                                android:textAlignment="center"
                                android:textColor="#131313"
                                android:textSize="16sp"
                                android:textStyle="bold" />
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="159dp"
                            android:layout_height="123dp"
                            android:layout_marginLeft="60dp"
                            android:layout_marginBottom="15dp"
                            android:orientation="vertical">

                            <ImageView
                                android:id="@+id/c4"
                                android:layout_width="120dp"
                                android:layout_height="92dp"
                                android:layout_gravity="center"
                                app:srcCompat="@drawable/motion_sensor" />

                            <TextView
                                android:id="@+id/textView23"

                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:text="Sensor C4"
                                android:textAlignment="center"
                                android:textColor="#131313"
                                android:textSize="16sp"
                                android:textStyle="bold" />
                        </LinearLayout>
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="horizontal">

                        <LinearLayout
                            android:layout_width="159dp"
                            android:layout_height="123dp"
                            android:layout_marginLeft="20dp"
                            android:layout_marginBottom="15dp"
                            android:orientation="vertical">

                            <ImageView
                                android:id="@+id/c5"
                                android:layout_width="match_parent"
                                android:layout_height="90dp"
                                app:srcCompat="@drawable/motion_sensor" />

                            <TextView
                                android:id="@+id/textVie2w2"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:text="Sensor C5"
                                android:textAlignment="center"
                                android:textColor="#131313"
                                android:textSize="16sp"
                                android:textStyle="bold" />
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="159dp"
                            android:layout_height="123dp"
                            android:layout_marginLeft="60dp"
                            android:layout_marginBottom="15dp"
                            android:orientation="vertical">

                            <ImageView
                                android:id="@+id/c6"
                                android:layout_width="120dp"
                                android:layout_height="92dp"
                                android:layout_gravity="center"
                                app:srcCompat="@drawable/motion_sensor" />

                            <TextView
